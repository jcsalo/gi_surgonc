	number = {1},
	journal = {Scand J Surg},
	author = {Kofoed, S. C. and Muhic, A. and Baeksgaard, L. and Jendresen, M. and Gustafsen, J. and Holm, J. and Bardram, L. and Brandt, B. and Brenø, J. and Svendsen, L. B.},
