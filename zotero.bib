  author = {Sugiyama, Keiji and Narita, Yukiya and Mitani, Seiichiro and Honda, Kazunori and Masuishi, Toshiki and Taniguchi, Hiroya and Kadowaki, Shigenori and Ura, Takashi and Ando, Masashi and Tajika, Masahiro and Muro, Kei},
