	number = {34},
	journal = {J Clin Oncol},
	author = {Leichman, Lawrence P. and Goldman, Bryan H. and Bohanes, Pierre O. and Lenz, Heinz J. and Thomas, Charles R. and Billingsley, Kevin G. and Corless, Christopher L. and Iqbal, Syma and Gold, Philip J. and Benedetti, Jacqueline K. and Danenberg, Kathleen D. and Blanke, Charles D.},
