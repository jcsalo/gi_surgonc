	pmid = {22334635},
	keywords = {Humans, Aged, Female, Male, Middle Aged, Neoadjuvant Therapy, Body Mass Index, Neoplasm Staging, Retrospective Studies, Risk Factors, Sex Factors, Overweight, Weight Loss, Aged, 80 and over, Adult, Adenocarcinoma, Carcinoma, Squamous Cell, Esophageal Neoplasms, Esophagectomy, Malnutrition},
	pages = {1088--1093},
	file = {Full Text:C\:\\Users\\jcsal\\Zotero\\storage\\T8SMT8EF\\Ouattara et al. - 2012 - Body mass index kinetics and risk factors of malnu.pdf:application/pdf},
