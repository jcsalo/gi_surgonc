  shorttitle = {Baseline {{18F-FDG PET}}/{{CT}} as Predictor of the Pathological Response to Neoadjuvant Therapy in Esophageal Cancer},
  author = {Domachevsky, Liran and Kashtan, Hanoch and Brenner, Baruch and Nidam, Meital and Morgenstern, Sara and Kundel, Yulia and Groshar, David and Bernstine, Hanna},
