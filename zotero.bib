	keywords = {Humans, Female, Male, Middle Aged, Treatment Outcome, Follow-Up Studies, Neoplasm Staging, Retrospective Studies, Disease Progression, Survival Rate, Prognosis, Stomach Neoplasms, Disease-Free Survival, Chemoradiotherapy, dCRT},
